{
  "name": "covid-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run copy\" \"npm run twCompiler\"  \"npm run ngCompiler\"",
    "copy": "ncp ./node_modules/@arcgis/core/assets ./src/assets/arcgis",
    "twCompiler": "tailwind -i src/app/lib/theme/tw/tailwind.scss -o src/app/lib/theme/tw/tailwind.dist.scss",
    "ngCompiler": "ng serve --host 0.0.0.0 --port 4220",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.3",
    "@angular/common": "^14.0.3",
    "@angular/compiler": "^14.0.3",
    "@angular/core": "^14.0.3",
    "@angular/forms": "^14.0.3",
    "@angular/material": "^14.0.3",
    "@angular/platform-browser": "^14.0.3",
    "@angular/platform-browser-dynamic": "^14.0.3",
    "@angular/router": "^14.0.3",
    "@arcgis/core": "^4.24.3",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "d3": "^7.5.0",
    "global": "^4.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "ncp": "^2.0.0",
    "postcss": "^8.4.14",
    "rxjs": "~7.5.0",
    "tailwindcss": "^3.1.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "^14.0.3",
    "@angular/compiler-cli": "^14.0.3",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.2"
  }
}